@startuml
  title DBモデリング1
  entity "メニュー" {
    メニューID
    ==
    名前
    値段
    種類
  }

  entity "注文" {
    注文ID
    ==
    注文者名
    注文者電話番号
    合計額
    支払い済か？
    注文日時
  }

  entity "注文詳細" {
    注文ID[FK]
    メニューID[FK]
    ==
    わさび有無
    注文個数
    小計額
  }

  注文 --|{ 注文詳細
  注文詳細 ||--|| メニュー
@enduml